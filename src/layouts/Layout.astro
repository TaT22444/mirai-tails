interface Props {
  title: string;
  URL?: string;
  href?: string;
  ogptitle?: string;
  type?: string;
  description?: string;
  sns_img?: string;
  paths: { label: string; url: string }[];
}

const { title, URL, ogptitle, type, description, sns_img, paths } = Astro.props;

