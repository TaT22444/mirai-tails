		<script type="module" defer>
			document.addEventListener('DOMContentLoaded', () => {
				const tagButtons = document.querySelectorAll('.header-aside-tagList-item-link');
		
				// タグボタンのクリックイベント
				tagButtons.forEach((button) => {
					button.addEventListener('click', (event) => {
						event.preventDefault(); // デフォルトのリンク動作を防ぐ
		
						const selectedTag = button.getAttribute('data-tag');
						const newUrl = `/?tag=${encodeURIComponent(selectedTag)}`;
		
						// pushState で URL を変更し、履歴を更新
						window.history.pushState({ tag: selectedTag }, '', newUrl);
		
						// ページを更新するためにリロード
						window.location.href = newUrl;
					});
				});
		
				// popstate イベントでブラウザの戻る/進む操作に対応
				window.addEventListener('popstate', (event) => {
					const selectedTag = event.state?.tag || '';
					const newUrl = `/?tag=${encodeURIComponent(selectedTag)}`;
					window.location.href = newUrl;
				});
			});
		</script>
		
		
